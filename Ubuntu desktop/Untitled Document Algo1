#include <iostream>
using namespace std;

int main() 
 {
	cin>>n;
	char a[20];
	for (int i=0;i<=n;i++)
	{ a[i]=i+1;
	};
	if(n%2==0);
	{for(int i=0;i<n;i=i+2)
	    { cout<<i+1<<i;
	        
	    }
	}
	if(n%2==1)
	{for(i=0;i<=3;i=i+2)
	   {cout<<i+1<<i;
	       
	   }
	   cout<<n-1<<n<<n-2;
	}
	
	return 0;
 }

